    "require": {
        "codemirror/codemirror": "^5.65",
        "composer/composer": "^2.8",
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^2.0",
        "drupal/ai_context": "^0.1@alpha",
        "drupal/ai_provider_litellm": "^1.2",
        "drupal/byte": "^1",
        "drupal/canvas": "^1.1",
        "drupal/core-composer-scaffold": "^11.3",
        "drupal/core-project-message": "^11.3",
        "drupal/core-recipe-unpack": "^11.3",
        "drupal/core-recommended": "^11.3",
        "drupal/core-vendor-hardening": "^11.3",
        "drupal/drupal_cms_accessibility_tools": "^2",
        "drupal/drupal_cms_ai": "^2",
        "drupal/drupal_cms_forms": "^2",
        "drupal/drupal_cms_google_analytics": "^2",
        "drupal/drupal_cms_seo_tools": "^2",
        "drupal/drupal_cms_starter": "^2",
        "drupal/entity_print": "^2.18",
        "drupal/json_ld_schema": "^2.0",
        "drupal/metatag": "^2.2",
        "drupal/recipe_installer_kit": "^1.0.4",
        "drupal/webform": "@beta",
        "jquery/inputmask": "^5.0",
        "jquery/intl-tel-input": "^17.0",
        "jquery/rateit": "^1.1",
        "jquery/select2": "^4.0",
        "jquery/textcounter": "^0.9.1",
        "jquery/timepicker": "^1.14",
        "popperjs/popperjs": "^2.11",
        "progress-tracker/progress-tracker": "^2.0",
        "signature_pad/signature_pad": "^2.3",
        "tabby/tabby": "^12.0",
        "tippyjs/tippyjs": "^6.3"
    },
    "require-dev": {
        "drush/drush": "^13.7",
        "drupal/core-dev": "^11.0",
        "phpstan/phpstan": "^2.0",
        "mglaman/phpstan-drupal": "^2.0",
        "phpstan/extension-installer": "^1.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-recipe-unpack": true,
            "drupal/core-vendor-hardening": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "bin-dir": "vendor/bin",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "patches.json": false,
                "patches.lock.json": false,
                "patches/drupal/drupal_cms/373.patch": false,
                "[web-root]/sites/default/settings.php": {
                    "path": "web/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                },
                "[web-root]/example.gitignore": false
            },
            "allowed-packages": ["drupal/drupal_cms_seo_tools"]
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "./drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "./recipes/{$name}": ["type:drupal-recipe"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": ["homepage", "support"],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                   </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed Drupal CMS!    </>",
                "<bg=blue;fg=white>                                                   </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://drupal.org/support",
                "  * Get involved with the Drupal community: https://drupal.org/getting-involved"
            ]
        },
        "drupal-core-vendor-hardening": {
            "composer/composer": false
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php -r \"@unlink('vendor/bin/composer');\""
        ],
        "post-drupal-scaffold-cmd": "cd web/sites/default && test -z \"$(grep 'include \\$devpanel_settings;' settings.php)\" && patch -Np1 -r /dev/null < $APP_ROOT/.devpanel/drupal-settings.patch || :"
    }
}
